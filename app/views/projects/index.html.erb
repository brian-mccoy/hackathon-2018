<div class="container">
  <div class="row">
    <div class="col-md-12">
      <h2>Projects</h2>
      <%= link_to 'New Project', new_project_path %>

      <% @projects.each do |project| %>
        <div>
          <%= link_to project.title, project_path(project) %>
        </div>
      <% end %>
    </div>
  </div>
</div>
